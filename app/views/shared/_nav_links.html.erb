<% if logged_in? %>
  <%= link_to current_user.username, user_path(current_user.id) %> | <%= link_to 'home', '/' %> | <%= link_to 'logout', logout_path, method: :delete %>
<% else %>
  <%= link_to 'home', '/' %> | <%= link to 'sign up', register_path %> | <%= link_to 'log in', login_path%>>
<% end %>
