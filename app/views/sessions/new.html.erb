<h4>Login</h4>
<%= render 'shared/form_errors' %>
<%= form_for(:session, url: login_path) do |f| %>
  <div>
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <%= f.submit "Log in", class: "button-primary" %>
<% end %>
